{
  "name": "blog",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check:type": "tsc  --project tsconfig.json -pretty --noEmit",
    "lint:es": "eslint . --ext ts,tsx,js",
    "lint:fix": "eslint . --ext ts,tsx,js --fix",
    "lint:style": "stylelint 'src/**/styled.ts' 'src/**/*.tsx'",
    "lint": "yarn lint:es && yarn lint:style",
    "format": "prettier --write .",
    "cache": "node scripts/cache.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run cache && yarn lint-staged && yarn check:type"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache"
    ],
    "styled.ts": [
      "stylelint --cache"
    ]
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.5.0",
    "@next/bundle-analyzer": "^10.0.4",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/typography": "^0.3.1",
    "@types/react-syntax-highlighter": "^13.5.0",
    "autoprefixer": "^10.1.0",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint-import-resolver-typescript": "^2.3.0",
    "expo-dark-mode-switch": "0.0.1",
    "gray-matter": "^4.0.2",
    "image-trace-loader": "^1.0.2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jimp": "^0.16.1",
    "lazysizes": "^5.3.0-beta1",
    "lodash": "^4.17.20",
    "lqip-loader": "^2.2.1",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^2.0.0",
    "next-optimized-images": "^2.6.2",
    "next-seo": "^4.17.0",
    "next-themes": "0.0.10",
    "postcss": "^8.2.1",
    "postcss-custom-properties": "^10.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "prism-react-renderer": "^1.1.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-live": "^2.2.3",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "react-toggle-dark-mode": "^1.0.3",
    "rehype-katex": "^4.0.0",
    "rehype-mathjax": "^3.0.0",
    "remark-html-katex": "^3.0.0",
    "remark-math": "3.0.1",
    "responsive-loader": "^2.3.0",
    "sass": "^1.32.0",
    "svg-sprite-loader": "^5.2.1",
    "tailwindcss": "^2.0.2",
    "typeface-merriweather": "^1.1.13",
    "typeface-open-sans": "^1.1.13",
    "webp-loader": "^0.6.0",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "devDependencies": {
